# Cut html files into game

ls *.html | xargs -P8 -i bash -c "csplit --digits=2  --quiet --prefix={} <(cat {} | htmlq '#gamelogs>div.gamelogreview' -t | grep -v 'to their preferences') '/is now the first player/+1' '{*}'"

# Get partner's name

ls /home/olivier/downloads/gl/*.html* | xargs -i sh -c 'head -n1 "{}" | grep -q Havanaa && sed -n 3p {} | cut -d" " -f1' | sort | uniq

# Extract links

var links = document.getElementsByTagName("a");
for(var i=0; i<links.length; i++)
  if(links[i].href.includes("table?table"))
    console.log(links[i].href);

